{
    "name": "domprojects/admin-ci",
    "description": "Template for the frontend and backend based on Bootstrap",
    "keywords": [
        "bootstrap",
        "template",
        "frontend",
        "backend",
        "codeigniter",
        "shield",
        "settings"
    ],
    "type": "library",
    "homepage": "https://domprojects.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Emmanuel Campait",
            "email": "support@domprojects.com",
            "homepage": "https://domprojects.com"
        }
    ],
    "support": {
        "issues": "https://github.com/domprojects/admin-ci/issues",
        "source": "https://github.com/domprojects/admin-ci"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "codeigniter4/framework": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "domprojects\\admin-ci\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
